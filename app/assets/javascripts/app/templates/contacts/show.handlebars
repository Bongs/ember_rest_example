<td>{{contact.id}}</td>
<td class="data">
  {{#if isEditing}}
    {{view App.EditContactView}}
  {{else}}
    {{contact.fullName}}
  {{/if}}
</td>
<td class="commands">
  {{#unless isEditing}}
    {{#view Ember.Link action="showEdit"}}Edit{{/view}}
    {{#view Ember.Link action="destroyRecord"}}Destroy{{/view}}
  {{/unless}}
</td>
