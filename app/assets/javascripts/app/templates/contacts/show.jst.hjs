<td>{{contact.id}}</td>
<td>
  {{#if isEditing}}
    {{view App.EditContactView contactBinding="contact"}}
  {{else}}
    {{contact.fullName}}
  {{/if}}
</td>
<td class="commands">
  {{#unless isEditing}}
    {{#view Ember.Link action="startEditing"}}Edit{{/view}}
    {{#view Ember.Link action="destroyRecord"}}Destroy{{/view}}
  {{/unless}}
</td>
